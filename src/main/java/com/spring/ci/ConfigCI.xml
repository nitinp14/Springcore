<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:c="http://www.springframework.org/schema/c"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.spring.ci.Certi" name="cer" c:name="python lang">
	
	</bean>

	<bean class="com.spring.ci.Person" name="person">
	<constructor-arg value="durgesh"></constructor-arg>
	<constructor-arg value="14" type="int"></constructor-arg>
	<constructor-arg ref="cer"></constructor-arg>
	</bean>
	
	<bean class="com.spring.ci.Addition" name="add">
	<constructor-arg value="12" type="double"></constructor-arg>
	<constructor-arg value="34" type="double"></constructor-arg></bean>

</beans>
