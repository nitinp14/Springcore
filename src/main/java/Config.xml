<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">
<!-- first bean -->
  <bean class="com.springcore.Student" name="student1">
    <property name="studentId">
      <value>1234</value>
    </property>
    <property name="studentName">
      <value>Ram</value>
    </property>
    <property name="studentAddress">
      <value>Blr</value>
    </property>
    </bean>

 <!-- second bean -->
     <bean class="com.springcore.Student" name="student2">
    <property name="studentId" value="9874"/>
    <property name="studentName" value="vipin"/>
    <property name="studentAddress" value="nagpur"></property>
  </bean>

 <!-- third bean -->
   <bean class="com.springcore.Student" name="student3" p:studentId="4567" p:studentName="virat" p:studentAddress="punjab"/>


</beans>
